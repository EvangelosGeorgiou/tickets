{
    "name": "evangeo/tickets",
    "description": "Ticket Support System",
    "keywords": [
        "evangeo/tickets",
        "laravel",
        "tickets"
    ],
    "homepage": "https://github.com/epls-tickets/tickets",
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2",
        "spatie/laravel-package-tools": "^1.14.0",
        "ext-json": "*",
        "illuminate/contracts": "^8.0|^9.0|^10",
        "illuminate/support": "^8.0|^9.0|^10",
        "laravel/tinker": "^2.7"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "classmap": [
            "database/seeds/",
            "database/factories/",
            "database/migrations/"
        ],
        "psr-4": {
            "EvanGeo\\Ticket\\": "src/",
            "EvanGeo\\Ticket\\Database\\Factories\\": "database/factories/",
            "EvanGeo\\Ticket\\Database\\Seeders\\": "database/seeds"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "database/seeds/",
            "database/factories/",
            "database/migrations/"
        ],
        "psr-4": {
            "EvanGeo\\Ticket\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EvanGeo\\Ticket\\Providers\\TicketsServiceProvider"
            ],
            "aliases": {
                "Tickets": "EvanGeo\\Ticket\\Facades\\Tickets"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
